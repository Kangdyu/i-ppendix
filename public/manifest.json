{
  "name": "i-ppendix",
  "description": "SKKU iCampus helper",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "static/js/background.js"
  },
  "action": {
    "default_title": "Click to open i-ppendix"
  },
  "permissions": ["activeTab", "cookies"],
  "host_permissions": ["https://canvas.skku.edu/*", "<all_urls>"],
  "icons": {
    "16": "i-ppendix-icon128.png",
    "32": "i-ppendix-icon128.png",
    "48": "i-ppendix-icon128.png",
    "128": "i-ppendix-icon128.png"
  }
}
